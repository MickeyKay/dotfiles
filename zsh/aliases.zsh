# git
alias gs="git status"
alias gl="git log --oneline --graph --decorate"
alias gp="git push"
alias gpf="git push --force-with-lease"
alias gc="git commit"
alias gco="git checkout"
alias gb="git branch"

# navigation
alias ..="cd .."
alias ...="cd ../.."

# general
alias ll="ls -lah"
alias c="clear"

# node / npm
alias ni="npm install"
alias nr="npm run"

# projects
alias cproj="cd \"$PROJECTS_DIR\""
